;    data     QUADDRAG 3 3  2.30E+06 ;
;    data     QUADDRAG 4 2  8.92E+06 ;
;    data     QUADDRAG 4 4  1.68E+10 ;
;    data     QUADDRAG 5 1 -8.92E+06 ;
;    data     QUADDRAG 5 5  1.68E+10 ;
